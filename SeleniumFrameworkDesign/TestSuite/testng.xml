<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Suite">

  <listeners>
   <listener class-name= "LearningAutomation.SeleniumFrameworkDesign.Testcomponents.Listeners"/>
  </listeners>
  
  <test thread-count="5" name="CorrectTest">
    <classes>
      <class name="LearningAutomation.SeleniumFrameworkDesign.Test.StandAlone"/>

  <test thread-count="5" name="ErrorTest">
    <classes>
      <class name="LearningAutomation.SeleniumFrameworkDesign.Test.ErrorValidation"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
